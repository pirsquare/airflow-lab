# Airflow Configuration - Copy to .env and customize

# Fernet key for encrypting connections/variables (generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())")
AIRFLOW__CORE__FERNET_KEY=your-fernet-key-here

# Webserver secret key for session management
AIRFLOW__WEBSERVER__SECRET_KEY=your-webserver-secret-key-here

# Admin user credentials
AIRFLOW_ADMIN_USER=admin
AIRFLOW_ADMIN_PASSWORD=admin
AIRFLOW_ADMIN_EMAIL=admin@example.com
